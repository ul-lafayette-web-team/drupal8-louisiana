{% if breadcrumb %}
  <div class="grid-container">
    <ol class="breadcrumbs">
    {% for item in breadcrumb %}
      <li>
        {% if item.url %}
          <a href="{{ item.url }}">{{ item.text }}</a>
        {% else %}
          {{ item.text }}
        {% endif %}
      </li>
    {% endfor %}
    </ol>
  </div>
{% endif %}

